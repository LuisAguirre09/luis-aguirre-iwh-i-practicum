doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Update Contact Info
    body 
        .form-wrapper
            p User: #{userEmail}
            p Video Game: #{videoGame}
            form(method="POST")
                label(for="game") Video Game
                input(type="text" name="game" required)
                input(type="submit" value="Submit")  
